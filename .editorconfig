# top-most EditorConfig file
root = true

[*]
indent_style = space
indent_size = 4
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true

[{.editorconfig,.gitattributes,.gitignore}]
end_of_line = lf
charset = utf-8

[{*.sln,*.csproj}]
indent_style = tab
tab_width = 4
end_of_line = crlf
charset = utf-8-bom

[{README.adoc,LICENSE}]
end_of_line = lf
charset = utf-8
